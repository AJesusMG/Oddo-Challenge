{
  "name": "odoo-challenge",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "sass --watch src/scss/main.scss dist/css/main.css",
    "build": "sass src/scss/main.scss dist/css/main.css --style=compressed",
    "vendor": "copyfiles -u 3 \"node_modules/bootstrap/dist/js/bootstrap.bundle.min.js\" dist/js",
    "sync": "copyfiles -u 2 \"src/js/main.js\" dist/js"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "gsap": "^3.14.2"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "sass": "^1.77.0"
  }
}
